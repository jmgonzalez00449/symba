(declare-const d0 Real)
  (declare-const d1 Real)
  (declare-const d2 Real)
  (declare-const d3 Real)  
  (declare-const d4 Real)  
  (declare-const t1 Real)  
  (declare-const t2 Real)  
  (declare-const tdelta Real)  
  (declare-const v1 Real)  
  (declare-const v2 Real)  
  (assert (=> 
           (and 
		(= d1 (* v1 t1))
		(= t1 5)
		(= d1 80)
		(>= d1 0)
		(< d1 100)
		(< v1 255)
		(> v1 0)
		
	 )
	 (and (< v1 255) (> v1 0) (< d1 100) (>= t1 1))
	)
  )
